#include <bits/stdc++.h>
using namespace std;

using ll = long long;
using ldb = long double;
using ui = unsigned;
using ull = unsigned long long;

ll power(int exp) {
    ll result = 1, base = 2;
    const ll mod = 1e9 + 7;
    while (exp > 0) {
        if (exp % 2 == 1)
            result = (result * base) % mod;
        base = (base * base) % mod;
        exp /= 2;
    }
    return result;
}

void solve() {
    int t;
    cin >> t;
    int n[t], k[t];
    for (int i = 0; i < t; i++) {
        cin >> n[i];
    }
    for (int i = 0; i < t; i++) {
        cin >> k[i];
        cout << power(k[i]) << '\n';
    }
    // for (int i = 0; i < t; i++) {
    // }
}

